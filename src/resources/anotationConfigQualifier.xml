<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
    <context:annotation-config/>

    <bean name="position" class="net.proselyte.springioc.annotationRunnerQualified.Position"/>

    <bean id="netJavaDeveloper" class="net.proselyte.springioc.annotationRunnerQualified.Developer">
        <constructor-arg index="0" value="Developer NET"/>
        <constructor-arg index="1" value="3"/>
        <constructor-arg index="2" value="Net Developer"/>
    </bean>

    <bean name="javaDeveloper" class="net.proselyte.springioc.annotationRunnerQualified.Developer">
        <constructor-arg index="0" value="Proffesional"/>
        <constructor-arg index="1" value="13"/>
        <constructor-arg index="2" value="JavaDeveloper"/>
    </bean>

</beans>