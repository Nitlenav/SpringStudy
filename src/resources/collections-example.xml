<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="collections"
          class="net.proselyte.springioc.collection.Collection">
        <property name="list">
            <list>
                <value>One</value>
                <value>Two</value>
                <value>Three</value>
                <value>Foure</value>
            </list>
        </property>
        <property name="set">
            <set>
                <value>One SET</value>
                <value>One SET</value>
                <value>Two SET</value>
                <value>Three SET</value>
                <value>Four SET</value>
            </set>
        </property>

        <property name="map">
            <map>
                <entry key="1" value="Onen MAP"/>
                <entry key="2" value="Two MAP"/>
                <entry key="2" value="Two MAP"/>
                <entry key="3" value="Three MAP"/>
                <entry key="4" value="Four MAP"/>
            </map>
        </property>

        <property name="properties">
            <props>
                <prop key="One">1_Props</prop>
                <prop key="Two">2_Props</prop>
                <prop key="Two">2_Props</prop>
                <prop key="Three">3_Props</prop>
                <prop key="Four">4_Props</prop>
            </props>
        </property>

    </bean>

</beans>